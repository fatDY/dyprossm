		<div class="content-wrapper">

			<!-- 内容头部 -->
			<section class="content-header">
			<h1>
				付款单据管理 <small>添加账户信息</small>
			</h1>
			<ol class="breadcrumb">
				<li><a href="${pageContext.request.contextPath}/index.jsp"><i
						class="fa fa-dashboard"></i> 首页</a></li>
				<li><a
					href="${pageContext.request.contextPath}/payment/findAll.do">付款单据管理</a></li>
				<li class="active">添加付款账户信息</li>
			</ol>
			</section>
			<!-- 内容头部 /-->

			<form
				action="${pageContext.request.contextPath}/payment/backToPaymentAdd.do"
				method="post">
				<!-- 正文区域 -->
				<section class="content">
				
					<table id="dataList"
							class="table table-bordered table-striped table-hover dataTable">
							<thead>
								<tr>
									<th class="" style="padding-right: 0px">
								</th>
									<th class="sorting_asc">ID</th>
									<th class="sorting_desc">账户名称</th>
									<th class="sorting_asc sorting_asc_disabled">账户号码</th>
									<th class="sorting_desc sorting_desc_disabled">账户性质</th>
									<th class="sorting">账户类别</th>
									<th class="text-center sorting">账户银行</th>
								</tr>
							</thead>
							<tbody>
							<div id="checkboxDiv">
								<c:forEach items="${pageInfo.list}" var="account">
									<tr>
										<td>
										
										<input name="ids" type="checkbox" value="${account.id}">
										</td>
										<td>${account.id }</td>
										<td>${account.accountName }</td>
										<td>${account.accountNo }</td>
										<td>${account.accountPurposeId }</td>
										<td>${account.accountType}</td>
										<td>${account.bankId.bankName}</td>
									</tr>
								</c:forEach>
								<input type="hidden" value="${paymentId}" name="paymentId">
							</div>
							</tbody>

						</table>
				<!--订单信息/--> <!--工具栏-->
				<div class="box-tools text-center">
					<button type="submit" class="btn bg-maroon">保存</button>
					<button type="button" class="btn bg-default"
						onclick="location.href='${pageContext.request.contextPath}/payment/paymentAddView.do'">返回</button>
				</div>
				<!--工具栏/--> </section>
				<!-- 正文区域 /-->
			</form>
			<div class="box-footer">
				<div class="pull-left">
					<div class="form-group form-inline" onchange="changePageSize()">
						总共${pageInfo.pages}页，共${pageInfo.total}条数据。 每页
						<select class="form-control" id="changePageSize">
							<option>5</option>
							<option>6</option>
							<option>7</option>
							<option>8</option>
							<option>9</option>
							<option>10</option>
						</select> 条
					</div>
				</div>

				<div class="box-tools pull-right">
					<ul class="pagination">
						<li><a href="${pageContext.request.contextPath}/payment/findAccountToPayment.do?page=1&size=${pageInfo.pageSize}" aria-label="Previous">首页</a></li>
						<li><a href="${pageContext.request.contextPath}/payment/findAccountToPayment.do?page=${pageInfo.pageNum-1}&size=${pageInfo.pageSize}">上一页</a></li>
						<c:forEach begin="1" end="${pageInfo.pages}" var="pageNum">
							<li><a href="${pageContext.request.contextPath}/payment/findAccountToPayment.do?page=${pageNum}&size=${pageInfo.pageSize}">${pageNum}</a></li>
						</c:forEach>

						<li><a href="${pageContext.request.contextPath}/payment/findAccountToPayment.do?page=${pageInfo.pageNum+1}&size=${pageInfo.pageSize}">下一页</a></li>
						<li><a href="${pageContext.request.contextPath}/payment/findAccountToPayment.do?page=${pageInfo.pages}&size=${pageInfo.pageSize}" aria-label="Next">尾页</a></li>
					</ul>
				</div>

			</div>
		</div>
